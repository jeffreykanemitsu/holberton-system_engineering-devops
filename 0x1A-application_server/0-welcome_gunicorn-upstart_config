description "Gunicorn application server running Airbnb"

start on runlevel [2345]
stop on runlevel [!2345]

respawn
setuid ubuntu
setgid www-data

env PATH=/home/ubuntu/AirBnB_clone_v2/airbnbenv/bin
chdir /home/ubuntu/AirBnB_clone_v2/web_flask
exec gunicorn --workers 3 --bind localHost:8001 web_flask.0-hello_route:app
